    "prettier": "^2.5.0",
    "socket.io": "^4.4.0",
    "swagger-ui-express": "^4.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.11",
    "@types/swagger-ui-express": "^4.1.3",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
