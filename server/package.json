{
  "name": "video-chat-app",
  "version": "1.0.0",
  "description": "Video chat app. Client + Server sides.",
  "main": "src/index.ts",
  "scripts": {
    "build": "npx rimraf dist/ && npx tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
